
LUA_DIR= c:\lua5.1\lua

LUAS= src\logging\logging.lua src\logging\console.lua src\logging\email.lua src\logging\file.lua src\logging\rolling_file.lua src\logging\socket.lua src\logging\sql.lua

build clean:

install:
	IF NOT EXIST $(LUA_DIR)\logging mkdir $(LUA_DIR)\logging
	FOR %i IN ($(LUAS)) DO copy %i $(LUA_DIR)\logging
